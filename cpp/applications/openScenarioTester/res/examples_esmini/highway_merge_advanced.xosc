      </Actions>
    </Init>
    <Story name="Highway_Merge_Story">
      <ParameterDeclarations>
        <ParameterDeclaration parameterType="string" name="$owner" value="Ego"/>
      </ParameterDeclarations>
      <Act name="EgoAct">
        <ManeuverGroup maximumExecutionCount="1" name="Highway_Merge_SequenceEgo">
          <Actors selectTriggeringEntities="false">
            <EntityRef entityRef="Ego"/>
          </Actors>
          <Maneuver name="EgoLaneChange">
            <Event name="EgoLaneChangeEvent" priority="overwrite">
              <Action name="EgoLaneChangeAction">
                <PrivateAction>
                  <LateralAction>
                    <LaneChangeAction targetLaneOffset="1">
                      <LaneChangeActionDynamics dynamicsShape="sinusoidal" value="2.8" dynamicsDimension="time"/>
                      <LaneChangeTarget>
                        <AbsoluteTargetLane value="-2"/>
                      </LaneChangeTarget>
                    </LaneChangeAction>
                  </LateralAction>
                </PrivateAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="EgoLaneChangeCondition" delay="0.0" conditionEdge="rising">
                    <ByEntityCondition>
                      <TriggeringEntities triggeringEntitiesRule="any">
                        <EntityRef entityRef="Ego"/>
                      </TriggeringEntities>
                      <EntityCondition>
                        <ReachPositionCondition tolerance="1.0">
                          <Position>
                            <LanePosition roadId="0" laneId="-3" s="20.0"/>
                          </Position>
                        </ReachPositionCondition>
                      </EntityCondition>
                    </ByEntityCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>
          </Maneuver>
        </ManeuverGroup>
        <StartTrigger>
          <ConditionGroup>
            <Condition name="" delay="0" conditionEdge="rising">
              <ByValueCondition>
                <SimulationTimeCondition value="0" rule="greaterThan"/>
              </ByValueCondition>
            </Condition>
          </ConditionGroup>
        </StartTrigger>
      </Act>
      <Act name="Act1">
        <ManeuverGroup maximumExecutionCount="1" name="Highway_Merge_SequenceA3">
          <Actors selectTriggeringEntities="false">
            <EntityRef entityRef="A3"/>
          </Actors>
          <Maneuver name="A3DistManuever">
            <Event name="A3CloseEvent" priority="overwrite">
              <Action name="A3CloseAction">
                <PrivateAction>
                  <LongitudinalAction>
                    <LongitudinalDistanceAction continuous="true" entityRef="A2" distance="7" freespace="false">
                      <DynamicConstraints maxAcceleration="10" maxDeceleration="7" maxSpeed="50"/>
                    </LongitudinalDistanceAction>
                  </LongitudinalAction>
                </PrivateAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="A3CloseGapStartCondition" delay="0.0" conditionEdge="rising">
                    <ByEntityCondition>
                      <TriggeringEntities triggeringEntitiesRule="any">
                        <EntityRef entityRef="A3"/>
                      </TriggeringEntities>
                      <EntityCondition>
                        <RelativeDistanceCondition entityRef="$owner" relativeDistanceType="lateral" value="3.25" freespace="false" rule="lessThan"/>
                      </EntityCondition>
                    </ByEntityCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>
            <Event name="A3OpenEvent" priority="overwrite">
              <Action name="A3OpenAction">
                <PrivateAction>
                  <LongitudinalAction>
                    <LongitudinalDistanceAction continuous="true" entityRef="A2" distance="20" freespace="false">
                      <DynamicConstraints maxAcceleration="10" maxDeceleration="7" maxSpeed="50"/>
                    </LongitudinalDistanceAction>
                  </LongitudinalAction>
                </PrivateAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="A3OpenGapStartCondition" delay="0.0" conditionEdge="rising">
                    <ByEntityCondition>
                      <TriggeringEntities triggeringEntitiesRule="any">
                        <EntityRef entityRef="A3"/>
                      </TriggeringEntities>
                      <EntityCondition>
                        <RelativeDistanceCondition entityRef="$owner" relativeDistanceType="lateral" value="3.25" freespace="false" rule="greaterThan"/>
                      </EntityCondition>
                    </ByEntityCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>
          </Maneuver>
        </ManeuverGroup>
        <StartTrigger>
          <ConditionGroup>
            <Condition name="ActStartCondition" delay="0" conditionEdge="rising">
              <ByEntityCondition>
                <TriggeringEntities triggeringEntitiesRule="all">
                  <EntityRef entityRef="A3"/>
                  <EntityRef entityRef="A2"/>
                </TriggeringEntities>
                <EntityCondition>
                  <RelativeDistanceCondition entityRef="$owner" relativeDistanceType="cartesianDistance" value="30" freespace="true" rule="lessThan"/>
                </EntityCondition>
              </ByEntityCondition>
            </Condition>
          </ConditionGroup>
        </StartTrigger>
        <StopTrigger>
          <ConditionGroup>
            <Condition name="ActEndCondDistLong" delay="0" conditionEdge="falling">
              <ByEntityCondition>
                <TriggeringEntities triggeringEntitiesRule="all">
                  <EntityRef entityRef="A3"/>
                  <EntityRef entityRef="A2"/>
                </TriggeringEntities>
                <EntityCondition>
                  <RelativeDistanceCondition entityRef="$owner" relativeDistanceType="cartesianDistance" value="30" freespace="true" rule="lessThan"/>
                </EntityCondition>
              </ByEntityCondition>
            </Condition>
          </ConditionGroup>
        </StopTrigger>
      </Act>
    </Story>
    <StopTrigger>
      <ConditionGroup>
        <Condition name="CutInActEnd" delay="0" conditionEdge="rising">
          <ByValueCondition>
            <SimulationTimeCondition value="0" rule="greaterThan"/>
          </ByValueCondition>
        </Condition>
      </ConditionGroup>
    </StopTrigger>
  </Storyboard>
</OpenSCENARIO>
